{
    "name": "papm-spreadsheet-service",
    "version": "0.0.1",
    "private": true,
    "description": "PaPM Spreadsheet Service",
    "author": "SAP",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "yarn lint --fix",
        "start": "nest start -e 'node -r browser-env/register'",
        "start:debug": "nest start --debug --watch -e 'node -r browser-env/register'",
        "start:dev": "nest start --watch -e 'node -r browser-env/register'",
        "start:prod": "NODE_ENV=prod node -r browser-env/register dist/main",
        "test": "cross-env NODE_ENV=test jest",
        "test:cov": "cross-env NODE_ENV=test jest --coverage",
        "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config test/jest-e2e.config.js --runInBand",
        "test:e2e:watch": "cross-env NODE_ENV=test yarn run test:e2e --watch --runInBand",
        "test:watch": "cross-env NODE_ENV=test jest --watch"
    },
    "resolutions": {
        "**/@types/node": "^12.12.37"
    },
    "dependencies": {
        "@grapecity/spread-sheets": "13.2.3",
        "@hapi/joi": "17.1.1",
        "@nestjs/common": "7.4.4",
        "@nestjs/config": "0.5.0",
        "@nestjs/core": "7.4.4",
        "@nestjs/platform-express": "7.4.4",
        "browser-env": "3.3.0",
        "canvas": "2.6.1",
        "class-transformer": "0.3.1",
        "class-validator": "0.12.2",
        "cookie-parser": "1.4.5",
        "csurf": "1.11.0",
        "express-basic-auth": "1.2.0",
        "fs-extra": "9.0.1",
        "lodash": "4.17.20",
        "nestjs-pino": "1.2.0",
        "pino-pretty": "4.3.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.5.1",
        "@nestjs/schematics": "^7.1.2",
        "@nestjs/testing": "^7.4.4",
        "@types/colors": "^1.2.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/csurf": "^1.9.36",
        "@types/express": "^4.17.8",
        "@types/fs-extra": "^9.0.2",
        "@types/hapi__joi": "^17.1.6",
        "@types/jest": "^26.0.14",
        "@types/lodash": "^4.14.162",
        "@types/node": "^14.11.8",
        "@types/node-fetch": "^2.5.7",
        "@types/redis": "^2.8.27",
        "@types/supertest": "^2.0.10",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.4.1",
        "@typescript-eslint/parser": "^4.4.1",
        "cross-env": "^7.0.2",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.1",
        "husky": "^4.3.0",
        "jest": "^26.5.3",
        "jest-junit": "^12.0.0",
        "lint-staged": "^10.4.0",
        "prettier": "^2.1.2",
        "prettier-plugin-packagejson": "^2.2.6",
        "start-server-and-test": "^1.11.5",
        "supertest": "5.0.0",
        "ts-jest": "^26.4.1",
        "ts-loader": "^8.0.5",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.3"
    },
    "engines": {
        "node": "12",
        "yarn": ">=1.22.0"
    }
}

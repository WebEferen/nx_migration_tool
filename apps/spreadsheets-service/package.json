{
    "name": "papm-spreadsheet-service",
    "version": "1.0.0",
    "private": true,
    "description": "PaPM Spreadsheet Service",
    "author": "SAP",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "yarn lint --fix",
        "start": "nest start -e \"node -r browser-env/register\"",
        "start:debug": "nest start --debug --watch -e \"node -r browser-env/register\"",
        "start:dev": "nest start --watch -e \"node -r browser-env/register\"",
        "start:prod": "node -r browser-env/register dist/main",
        "test": "cross-env NODE_ENV=test jest --selectProjects unit-tests",
        "test:cov": "yarn run test --coverage",
        "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --selectProjects unit-tests --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --selectProjects e2e --runInBand",
        "test:e2e:watch": "yarn run test:e2e --watch",
        "test:watch": "yarn run test --watch"
    },
    "resolutions": {
        "typescript": "4.2.4"
    },
    "dependencies": {
        "@dynatrace/oneagent": "1.219.134-1.0.0",
        "@grapecity/spread-sheets": "14.0.10",
        "@nestjs/common": "7.6.15",
        "@nestjs/config": "0.6.3",
        "@nestjs/core": "7.6.15",
        "@nestjs/platform-express": "7.6.15",
        "@nestjs/swagger": "4.8.0",
        "@sap/xsenv": "3.1.0",
        "@sap/xssec": "3.1.0",
        "browser-env": "3.3.0",
        "canvas": "2.7.0",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "express": "4.17.1",
        "express-basic-auth": "1.2.0",
        "fs-extra": "10.0.0",
        "joi": "17.4.0",
        "lodash": "4.17.21",
        "nestjs-pino": "1.4.0",
        "passport": "0.4.1",
        "pino-pretty": "4.7.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.7",
        "swagger-ui-express": "4.1.6"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@nestjs/testing": "^7.6.15",
        "@types/colors": "^1.2.1",
        "@types/cookie-parser": "^1.4.2",
        "@types/csurf": "^1.11.1",
        "@types/express": "^4.17.11",
        "@types/fs-extra": "^9.0.11",
        "@types/jest": "^26.0.23",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/lodash": "^4.14.168",
        "@types/node": "^15.0.2",
        "@types/node-fetch": "^2.5.10",
        "@types/node-jose": "^1.1.6",
        "@types/passport": "^1.0.6",
        "@types/redis": "^2.8.28",
        "@types/sap__xsenv": "^2.0.2",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "cross-env": "^7.0.3",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "husky": "^4.3.0",
        "jest": "^26.6.3",
        "jest-junit": "^12.0.0",
        "jsonwebtoken": "^8.5.1",
        "lint-staged": "^10.5.4",
        "node-jose": "^2.0.0",
        "prettier": "^2.2.1",
        "prettier-plugin-packagejson": "^2.2.11",
        "start-server-and-test": "^1.12.1",
        "supertest": "6.1.3",
        "ts-jest": "^26.5.6",
        "ts-loader": "^9.1.2",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4"
    },
    "engines": {
        "node": "^15 || ^16",
        "yarn": ">=1.22.0"
    }
}
